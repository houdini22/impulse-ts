{
  "name": "impulse-ts",
  "version": "0.0.2",
  "description": "TypeScript Neural Network with GPU computation.",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/node": "^14.17.12",
    "babel-loader": "^8.2.2",
    "node-loader": "^2.0.0",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  },
  "scripts": {
    "build-dev": "webpack --config ./configs/dev.js",
