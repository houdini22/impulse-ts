{
  "name": "impulse-ts",
  "version": "0.0.2",
  "description": "TypeScript Neural Network with GPU computation.",
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "node-loader": "^2.0.0",
    "prettier": "^2.4.0",
    "typescript": "^4.4.3",
    "webpack": "^5.51.2",
    "webpack-cli": "^4.8.0"
  },
  "scripts": {
    "build-dev": "webpack --config ./configs/dev.js",
    "build-prod": "webpack --config ./configs/prod.js",
    "prettier-write": "prettier --write \"src/typescript/**/*.tsx\" && prettier --write \"examples/**/*.js\"",
    "preinstall": "node-gyp clean || true; node-gyp --debug configure build",
    "install": "node-gyp rebuild --debug",
    "clean-dist": "rm -f -r -d ./dist/*",
    "lint": "eslint . --ext .tsx",
    "lint-fix": "eslint . --ext .tsx --fix"
  },
  "license": "Copyright Micha≈Ç Baniowski",
  "dependencies": {
    "csvtojson": "^2.0.10",
    "gpu.js": "^2.11.4",
    "node-gyp": "^8.2.0"
  },
  "main": "./dist/impulse-ts.js"
}
